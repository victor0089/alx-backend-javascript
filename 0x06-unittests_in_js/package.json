{
  "name": "unittests_in_js",
  "version": "1.0.0",
  "description": "A project for learning the basics of unittests in Node.js.",
  "author": "Bezaleel Olakunori <bezaleeloci@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint",
    "lint-all": "./node_modules/.bin/eslint [0-9]*.js",
    "lint-fix-all": "./node_modules/.bin/eslint --fix [0-9]*.js",
    "test": "node ./node_modules/mocha/bin/mocha --require babel-register --exit",
    "dev": "nodemon --exec babel-node --presets babel-preset-env ./full_server/server.js ./database.csv"
  },
  "dependencies": {
    "chai-http": "^4.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "request": "^2.88.0",
    "sinon": "^7.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/B3zaleel/alx-backend-javascript"
  },
  "bugs": {
    "url": "https://github.com/B3zaleel/alx-backend-javascript/issues"
